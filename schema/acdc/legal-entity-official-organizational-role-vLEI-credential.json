              "required": [
                "n",
                "s",
                "o"
              ]
            }
          },
          "additionalProperties": false,
          "required": [
            "d",
            "auth"
          ]
        }
      ]
    },
    "r": {
      "oneOf": [
        {
          "description": "Rules block SAID",
          "type": "string"
        },
